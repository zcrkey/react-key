{
  "name": "react-key",
  "version": "0.0.1",
  "description": "UI Library Based on react.",
  "main": "lib/index.js",
  "scripts": {
    "yarn": "yarn",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack",
    "build:npm": "cross-env NODE_ENV=production-npm webpack",
    "link:npm": "yarn link",
    "unlink:npm": "yarn unlink",
    "jsdoc": "jsdoc -c jsdoc.config.js -r"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcrkey/react-key.git"
  },
  "keywords": [
    "react",
    "ui"
  ],
  "author": "zcr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zcrkey/react-key/issues"
  },
  "homepage": "https://github.com/zcrkey/react-key#readme",
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.6.3",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.13.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^8.0.0",
    "tui-jsdoc-template": "^1.2.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
